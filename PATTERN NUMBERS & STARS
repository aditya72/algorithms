#include<iostream>
using namespace std;
int main()
{
int n,i,j,k;
cin>>n;
int temp=n-1;
for(i=1;i<=n;i++)
{
    for(j=1;j<=i;j++)
    {
        cout<<j;
    }
    if(i<n)
    {
        for(k=1;k<=temp;k++)
        {
            cout<<"*";
        }
        temp--;
    }
    cout<<endl;
}
return 0;
}
